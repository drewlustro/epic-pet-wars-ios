$api_keys = array();
$api_secrets = array();

function display_obj_c($name, $api_key, $api_secret, $first) {
  if ($first) {
    echo "#ifdef ";
  } else {
    echo "#elif ";
  }
  
  echo "$name\n\n#define API_KEY @\"$api_key\"\n#define APP_SECRET @\"$api_secret\"\n\n";
}

$first = true;
foreach ($packs as $pack) {
  $api_key = md5($version . $pack[0] . $pack[1] . microtime() . rand());
  $api_secret = md5($version . $pack[1] . $pack[0] . microtime() . rand());
  
  $api_keys[] = $api_key;
  $api_secrets[] = $api_secret;
  
  display_obj_c($pack[0], $api_key, $api_secret, $first);
  
  $first = false;
}

echo "#endif\n\n";

for ($i = 0; $i < count($packs); $i += 1) {
  $api_key = $api_keys[$i];
  $api_secret = $api_secrets[$i];
  $pack = $packs[$i][1];
  
  echo "\"$api_key\" => array(\"$api_secret\", \"$pack\", \"$version\"),\n";
}
